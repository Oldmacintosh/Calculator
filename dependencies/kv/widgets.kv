<RightAlignedTextButton>: 
    text_size: self.size
    halign: 'right'
    valign: 'middle'
    background_normal: ''
    background_down: ''
    always_release: True
<LabelLikeTextInput>: 
    halign: 'center'
    background_color: [0, 0, 0, 0]
    cursor_blink: False
    cursor_width: 0
    readonly: True
    multiline: False
    padding_y: [self.height / 2.0 - (self.line_height / 2.0) * len(self._lines), 0]
<SM>: 
    transition: NoTransition()
    EmptyScreen: 
    Standard: 
    Quadratic: 
    _Settings: